{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "allowJs": true,
        "sourceMap": false,
        "outDir": "./builds",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": [
            "node_modules/@types"
        ]
    },
    "exclude": [
        "./node_modules",
        "./buildc",
    ],
    "include": [
        "./server"
    ]
}